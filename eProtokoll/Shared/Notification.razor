@inject INotificationService _service;
<RadzenMenu>
    <RadzenMenuItem Text="" Icon="account_box">
        @foreach (var i in Notifications)
        {
            <RadzenMenuItem Text="@i.message" Path="panel" Icon="content_paste"></RadzenMenuItem>
        }
    </RadzenMenuItem>

</RadzenMenu>

@code {

    private List<NotificationDto> Notifications = new();
    protected override async Task OnInitializedAsync()
    {
        Notifications = await _service.GetNotifications();
    }

    async Task OnClick()
    {
    }
    private bool collapseNavMenu = true;


}
